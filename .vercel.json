{
  "version": 2,
  "routes": [
    { "handle": "filesystem" },
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "/_next/static/(.*)", "dest": "/_next/static/$1" },
    { "src": "/_next/image(.*)", "dest": "/_next/image$1" },
    { "src": "/_next/(.*)", "dest": "/_next/$1" },
    { "src": "/(.+)\\.(jpg|jpeg|png|gif|ico|webp|svg|css|js|json|woff|woff2|ttf|eot)", "dest": "/$1.$2" },
    { "src": "/(.*)", "dest": "/index" }
  ],
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ]
}